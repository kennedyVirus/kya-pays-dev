RewriteEngine On

RewriteCond %{HTTPS} off

RewriteRule ^(.*)$ https://kya-pay-dev.kya-energy.com/$1 [L,R=301]

RewriteCond %{HTTP_HOST} ^www\.(([a-z0-9_]+\.)?kya-pay-dev.kya-energy\.com)$ [NC]

RewriteRule .? http://%1%{REQUEST_URI} [R=301,L]

RewriteCond %{REQUEST_FILENAME} !-f RewriteRule ^(.*)$ web/$1 [QSA,L]